{
  "name": "hyperdrive.client",
  "version": "0.0.0",
  "description": "Solución para gestionar el almacenamiento, visualización y descarga de ficheros.",
  "author": {
    "name": "Javier Pardo",
    "email": ""
  },
  "main": "electron.main.js",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf dist",
    "web": "ng serve --ssl",
    "desktop": "ng serve --ssl | electron .",
