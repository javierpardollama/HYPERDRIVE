{
  "$schema": "https://json.schemastore.org/appsettings.json",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Jwt": {
    "JwtAudience": "",
    "JwtExpireMinutes": 60,
    "JwtExpireDays": 2,
    "JwtIssuer": "",
    "JwtAuthority": "",
    "JwtKey": ""
  },
  "RateLimit": {
    "PolicyName": "Concurrency",
    "PermitLimit": 20,
    "QueueProcessingOrder": 0,
    "QueueLimit": 10
  },
  "Rabbit": {
    "Url": "",
    "Queue": "",
    "User": "",
    "Key": "",
    "Mandatory": true,
    "ClientProvider": "",
    "AutomaticRecoveryEnabled": true,
    "NetworkRecoveryInterval": 5,
    "TopologyRecoveryEnabled": true,
    "PublisherConfirmationsEnabled": true,
    "PublisherConfirmationTrackingEnabled": true,
    "Durable": true,
    "Exclusive": true,
    "AutoDelete": true
  }
} 

