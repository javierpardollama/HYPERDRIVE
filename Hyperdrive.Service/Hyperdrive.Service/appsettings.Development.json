{
  "$schema": "https://json.schemastore.org/appsettings.json",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=hyperdrive.db"
  },
  "Jwt": {
    "JwtAudiences": [ "https://localhost:4200" ],
    "JwtExpireMinutes": 60,
    "JwtIssuer": "https://localhost:7297/",
