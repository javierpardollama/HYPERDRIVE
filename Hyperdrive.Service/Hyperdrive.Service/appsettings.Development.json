    "JwtKey": "SOME_RANDOM_KEY_DO_NOT_SHARE"
  },
  "RateLimit": {
    "PolicyName": "fixed",
    "PermitLimit": 4,
    "Window": 12,
    "QueueProcessingOrder": 0,
    "QueueLimit": 2
  }
}